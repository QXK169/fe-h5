  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vant"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "Chrome >= 51",
    "iOS >= 10"
  ],
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "^1.3.4",
