{
  "name": "fe-h5",
  "version": "1.0.3",
  "description": "前端H5包",
  "main": "lib/fe-H5.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "dev": "vant-cli dev",
    "lint": "vant-cli lint",
    "test": "vant-cli test",
    "build": "vant-cli build",
    "build:site": "vant-cli build-site && node deploy.js",
    "release": "vant-cli release",
